---
 import Root from '../../components/Root.astro';
import { defaultLocale } from '../../constants';
import { localeKeys } from '../../lib/locales';

export const getStaticPaths = (() => {
	return localeKeys
		.filter(locale => locale !== defaultLocale)
		.reduce((paths, locale) => {
		return paths.concat({
			params: {
				lang: locale
			}
		})
	}, [])
});
---
<Root />